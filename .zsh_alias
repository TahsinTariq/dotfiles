# aliases for zsh


# add aliases here
# to remove aliase:
#       unalias [alias name]

alias hl='echo 'hellow''
alias js='sudo jekyll serve'
alias czs='cat $ZSH_CUSTOM/.zsh_alias'
alias nzs='nano $ZSH_CUSTOM/.zsh_alias'
